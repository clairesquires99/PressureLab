# Auth Configuration
# Set to 'true' to enable Clerk authentication, 'false' to disable
VITE_AUTH_ENABLED=false

# Clerk Configuration (only needed if VITE_AUTH_ENABLED=true)
# Get these from https://dashboard.clerk.com
VITE_CLERK_PUBLISHABLE_KEY=your_clerk_publishable_key_here

# Reserved User ID (used when auth is disabled)
VITE_RESERVED_USER_ID=guest-user-000
